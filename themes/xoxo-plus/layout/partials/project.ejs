<section class="post page archives">
  <h3 class="archive-year-wrap"><%= __('project')%></h3>
  <div class="archives">
    <% Object.keys(theme.projects).forEach(function(key) { %>
      <div class="archive">
        <a class="post-go" target="_blank" style="color:#202020" href="<%=theme.projects[key]['url']%>">
          <%= key%><span class="slash">/</span><span class="date" style="text-transform:none;font-size:14px;"><%=theme.projects[key]['descript']%></span>
        </a>
      </div>
    <% }); %>
    <p style="color:#999;font-size:14px;margin-top: 30px;">整理更新中...</p>
  </div>
</section>
