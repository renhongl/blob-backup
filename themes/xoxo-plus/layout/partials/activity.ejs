<section class="post page archives">
  <h3 class="archive-year-wrap"><%= __('activity')%></h3>
  <div class="archives">
    <% Object.keys(theme.activitys).forEach(function(key) { %>
      <div class="archive">
        <a class="post-go" target="_blank" style="color:#202020" href="<%=theme.activitys[key]['url']%>">
          <span class="date"><%- date(theme.activitys[key]['date'], 'YYYY-MM-DD') %></span><span class="slash">/</span>[<%=theme.activitys[key]['type']%>] <%= key%>
        </a>
      </div>
    <% }); %>
    <p style="color:#999;font-size:14px;margin-top: 30px;">整理更新中...</p>
  </div>
</section>
